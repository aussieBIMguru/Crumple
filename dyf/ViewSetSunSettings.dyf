      "NodeType": "PythonScriptNode",
      "Code": "# Made by Gavin Crump\r\n# Free for use\r\n# BIM Guru, www.bimguru.com.au\r\n\r\n# Credit to Matthew Anderle & Chris Williams for their examples\r\n# DTx Library: https://trello.com/c/mOujiMPA/39-slider-sun-settings\r\n\r\n# Boilerplate text\r\nimport clr\r\n\r\nimport System\r\n\r\nclr.AddReference(\"RevitNodes\")\r\nimport Revit\r\nclr.ImportExtensions(Revit.Elements)\r\nclr.ImportExtensions(Revit.GeometryConversion)\r\n\r\nclr.AddReference(\"RevitServices\")\r\nimport RevitServices\r\nfrom RevitServices.Persistence import DocumentManager \r\nfrom RevitServices.Transactions import TransactionManager \r\n\r\nclr.AddReference(\"RevitAPI\")\r\nimport Autodesk \r\nfrom Autodesk.Revit.DB import *\r\n\r\n# Current doc/app/ui\r\ndoc = DocumentManager.Instance.CurrentDBDocument\r\n\r\n# To list function\r\ndef tolist(input):\r\n\tout = input if isinstance(input, list) else [input]\r\n\treturn out\r\n\r\n# Preparing input from dynamo to revit\r\nviews = tolist(IN[0])\r\ntimes = tolist(IN[1])\r\nshade = IN[2]\r\nlevel = UnwrapElement(IN[3])\r\nno_gp = IN[4]\r\n\r\n# Datetimes set as local\r\nDT_kind = System.DateTimeKind.Local\r\n\r\n# Rebuild datetimes list\r\nDT_times = []\r\nfor time in times:\r\n\tspectime = System.DateTime.SpecifyKind(time, DT_kind)\r\n\tDT_times.append(spectime)\r\n\r\n# Do some action in a Transaction\r\nTransactionManager.Instance.EnsureInTransaction(doc)\r\n\r\n# Transactable actions\r\nviews_done = []\r\n\r\nfor view, time in zip(views, DT_times):\r\n\tview_uw  = UnwrapElement(view)\r\n\tsettings = view_uw.SunAndShadowSettings\r\n\tview_uw.ShadowIntensity = 1\r\n\tsettings.SunAndShadowType = SunAndShadowType.StillImage\r\n\tsettings.StartDateAndTime = time\r\n\tview_uw.ShadowIntensity = shade\r\n\tif no_gp:\r\n\t\tsettings.UsesGroundPlane = False\r\n\telse:\r\n\t\tsettings.UsesGroundPlane = True\r\n\t\tsettings.GroundPlaneLevelId = level.Id\r\n\tviews_done.append(view)\r\n\r\n# Do some action in a Transaction\r\nTransactionManager.Instance.TransactionTaskDone()\r\n\r\n# Preparing output to Dynamo\r\nOUT = [views_done, DT_times]",
