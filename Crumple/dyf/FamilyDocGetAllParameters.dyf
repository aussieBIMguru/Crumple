      "NodeType": "PythonScriptNode",
      "Code": "# Made by Gavin Crump\r\n# Free for use\r\n# BIM Guru, www.bimguru.com.au\r\n\r\n# Boilerplate text\r\nimport clr\r\n\r\nclr.AddReference(\"RevitServices\")\r\nimport RevitServices\r\nfrom RevitServices.Persistence import DocumentManager \r\n\r\nclr.AddReference('RevitAPI')\r\nimport Autodesk \r\nfrom Autodesk.Revit.DB import FamilyManager, Parameter\r\n\r\n# Get Revit build version\r\nuiapp = DocumentManager.Instance.CurrentUIApplication\r\napp = uiapp.Application\r\nver = int(app.VersionNumber)\r\n\r\nif ver > 2023:\r\n\tfrom Autodesk.Revit.DB import ParameterUtils\r\n\r\n# Define list/unwrap list functions\r\ndef tolist(input):\r\n    result = input if isinstance(input, list) else [input]\r\n    return result\r\n\r\ndef parameterGroup(par):\r\n\tparGrp = param.Definition.ParameterGroup\r\n\tif ver > 2023:\r\n\t\treturn ParameterUtils.GetParameterGroupTypeId(parGrp)\r\n\telse:\r\n\t\treturn parGrp\r\n\r\n#Inputdoc\r\ndocs_list = tolist(IN[0])\r\n\r\n# Define lists\r\nparamLists, nameLists, grpLists, sharedLists, instLists = [],[],[],[],[]\r\n\r\n# For each document...\r\nfor doc in docs_list:\r\n\tparamList, nameList, grpList, sharedList, instList = [],[],[],[],[]\r\n\t# If a family document...\r\n\tif doc.IsFamilyDocument:\r\n\t\t# For each parameter...\r\n\t\tfor param in doc.FamilyManager.Parameters:\r\n\t\t\t# Add its data\r\n\t\t\tparamList.append(param)\r\n\t\t\tnameList.append(param.Definition.Name)\r\n\t\t\tgrpList.append(parameterGroup(param))\r\n\t\t\tsharedList.append(param.IsShared)\r\n\t\t\tinstList.append(param.IsInstance)\r\n\t# Append the lists\r\n\tparamLists.append(paramList)\r\n\tnameLists.append(nameList)\r\n\tgrpLists.append(grpList)\r\n\tsharedLists.append(sharedList)\r\n\tinstLists.append(instList)\r\n\r\n# Output the lists\r\nOUT = docs_list, paramLists, nameLists, grpLists, sharedLists, instLists",
