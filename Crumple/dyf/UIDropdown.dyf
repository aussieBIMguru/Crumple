      "NodeType": "PythonScriptNode",
      "Code": "# Made by Gavin Crump\r\n# Free for use\r\n# BIM Guru, www.bimguru.com.au\r\n\r\nimport clr\r\n\r\nclr.AddReference(\"System.Drawing\")\r\nclr.AddReference(\"System.Windows.Forms\")\r\n\r\nimport System\r\nimport System.Drawing\r\n\r\nfrom System.Drawing import *\r\nfrom System.Windows.Forms import *\r\n\r\nfrom collections import namedtuple\r\n\r\n# Define list/unwrap list functions\r\ndef tolist(input):\r\n    result = input if isinstance(input, list) else [input]\r\n    return result\r\n\r\n# Imports from Dynamo\r\nkeys      = tolist(IN[1])\r\nvalues    = tolist(IN[2])\r\nformTitle = IN[0]\r\n\r\nformWidth  = 300\r\nformHeight = 100\r\nbuttonHeight = 225\r\n\r\n# Class for key/value pair\r\nclass MyItem():\r\n\tdef __init__(self, key, value):\r\n\t\tself.Key = key\r\n\t\tself.Value = value\r\n\r\n# Make key value pairs\r\nlist_of_items = [MyItem(i,j) for i,j in zip(keys, values)] \r\n\r\n# Class for main winform\r\nclass MainForm(Form):\r\n\t\r\n\t# Initialize form object\r\n\tdef __init__(self):\r\n\t\t# Begin with first value\r\n\t\tself.choice = list_of_items[0].Value\r\n\t\tself.InitializeComponent()\r\n\t\r\n\t# Make form components\r\n\tdef InitializeComponent(self):\r\n\t\t\r\n\t\t# Combo box\r\n\t\tself._comboBoxType = System.Windows.Forms.ComboBox()\r\n\t\tself._comboBoxType.Location = System.Drawing.Point(25, 25)\r\n\t\tself._comboBoxType.Size = System.Drawing.Size(formWidth-50, 25)\r\n\t\tself._comboBoxType.DataSource = list_of_items\r\n\t\tself._comboBoxType.DisplayMember = \"Key\"\r\n\t\tself._comboBoxType.ValueMember = \"Value\"\r\n\t\tself._comboBoxType.SelectedIndexChanged += self.newItemSelected\r\n\t\tself.Controls.Add(self._comboBoxType)\r\n\t\t\r\n\t\t# Text box\r\n\t\tself.tbox1 = TextBox()\r\n\t\tself.tbox1.Parent = self\r\n\t\tself.tbox1.Text = \"test\"\r\n\t\tself.tbox1.Location = Point(25,25)\r\n\t\tself.tbox1.Width = 100\r\n\t\t\r\n\t\t# OK button\r\n\t\tbtnOk = Button()\r\n\t\tbtnOk.Text = \"OK\"\r\n\t\tbtnOk.Anchor = (AnchorStyles.Bottom | AnchorStyles.Right)\r\n\t\tbtnOk.Location = Point(105, buttonHeight)\r\n\t\tself.Controls.Add(btnOk)\r\n\t\tbtnOk.Click += self.okButtonPressed\r\n\r\n\t\t# Cancel button\r\n\t\tbtnCancel = Button()\r\n\t\tbtnCancel.Text = \"Cancel\"\r\n\t\tbtnCancel.Anchor = (AnchorStyles.Bottom | AnchorStyles.Right)\r\n\t\tbtnCancel.Location = Point(185, buttonHeight)\r\n\t\tself.Controls.Add(btnCancel)\r\n\t\tbtnCancel.Click += self.cancelButtonPressed\r\n\t\t\r\n\t\t# Draw main form\r\n\t\tself.ClientSize = System.Drawing.Size(formWidth, formHeight)\r\n\t\tself.BringToFront()\r\n\t\tself.CenterToScreen()\r\n\t\tself.FormBorderStyle = FormBorderStyle.FixedSingle\r\n\t\tself.Name = formTitle\r\n\t\tself.Text = formTitle\r\n\t\tself.ResumeLayout(False)\r\n\t\r\n\t# Event if OK button pressed\r\n\tdef okButtonPressed(self, sender, args):\r\n\t\tself.Close()\r\n\t\tself.runNextOutput = True\r\n\r\n\t# Event if OK button pressed\r\n\tdef cancelButtonPressed(self, sender, args):\r\n\t\tself.choice = None\r\n\t\tself.Close()\r\n\t\tself.runNextOutput = False \r\n\r\n\t# Event if dropdown changes\r\n\tdef newItemSelected(self, sender, e):\r\n\t\tself.choice = sender.SelectedItem.Value\r\n\t\tself.runNextOutput = False\r\n\r\n# Show form to the user\r\nobjForm = MainForm()\r\nobjForm.ShowDialog()\r\n\r\n# Return the selected object and if it was cancelled\r\nchosenItem = objForm.choice\r\n\r\nif not chosenItem:\r\n\tOUT = None, True\r\nelse:\r\n\tOUT = chosenItem, False",
